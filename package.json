{
  "name": "apiviz",
  "version": "1.0.0",
  "description": "A neat way to visualize an API response",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "server": "node server.js",
    "client": "node utils/client.js",
    "test": "cross-env NODE_ENV=test mocha",
    "cover": "nyc --reporter=lcov --reporter=text-summary npm test",
    "seed": "node utils/seed-database.js",
    "proxy": "concurrently \"npm run server\" \"npm run client\"",
    "hash": "node utils/hash-password.js",
    "docs": "aglio -i apiHub.apib -o docs.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnatspreadstreet/boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnatspreadstreet/boilerplate/issues"
  },
  "homepage": "https://github.com/johnatspreadstreet/boilerplate#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-force-https": "^1.0.0",
    "express-http-proxy": "^1.5.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.5",
    "morgan": "^1.9.1",
    "papaparse": "^4.6.3",
    "parseurl": "^1.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path-match": "^1.2.4",
    "path-to-regexp": "^3.0.0",
    "query-string": "^6.2.0",
    "quicktype": "^15.0.181",
    "redux-thunk": "^2.3.0",
    "uri-templates": "^0.2.0",
    "urijs": "^1.19.1",
    "url-parse": "^1.4.4",
    "wurl": "^2.5.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "4.0.0",
