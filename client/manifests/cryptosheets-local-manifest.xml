                    <bt:Image size="64" resid="feature-request-icon-64"/>
                    <bt:Image size="80" resid="feature-request-icon-80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>openFeatureRequests</FunctionName>
                  </Action>
                </Control>
              </Group>
              <Label resid="environment-name"/>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
