  <Id>59a971ff-c423-40ca-9aaf-c5424124c46c</Id>
  <Version>1.0.0.8</Version>
  <ProviderName>Cryptosheets</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Cryptosheets Local"></DisplayName>
  <Description DefaultValue="Cryptosheets simplifies the process of accessing, aggregating, manipulating, and processing cryptocurrency data."></Description>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"></IconUrl>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/hi-res-icon.png"></HighResolutionIconUrl>
  <SupportUrl DefaultValue="https://cryptosheets.com/support"></SupportUrl>
  <AppDomains>
    <AppDomain>https://accounts.google.com/signin/oauth</AppDomain>
    <AppDomain>https://localhost:8080</AppDomain>
    <AppDomain>https://884d7f77.ngrok.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"></Host>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="ExcelApi" MinVersion="1.1"></Set>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/app"></SourceLocation>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
